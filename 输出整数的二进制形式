#include <stdio.h>
#include <math.h>
void putBinary(int n);
int main() {
    int n;
    while (scanf(" %d", &n) != EOF){
    putBinary(n);
    }
}
void putBinary(int n)
{
    int bits = sizeof(n) * 8;
    while (bits-- > 0) {
        printf("%d", n >> bits & 1);
        if (bits % 8 == 0) printf(" ");
    }

    printf("<->%d\n",n);
}
